
#include <iostream>

using namespace std;

int main() 
{   int a[10000000],b[10000000],c[10000000],n,j,i,k;
    cin>>n;
    for(j=0;j<n;j++)
    {   cin>>a[j];
    }
    for(k=0;k<n;k++)
    {   cin>>b[k];  }
    
    for( i=0;i<n;i++)
    {   c[i]=a[i]+b[i];     
        cout<<c[i]<<" ";
    }
    return 0;
}
    
